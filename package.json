{
  "name": "activity-tracker",
  "version": "0.0.0",
  "scripts": {
    "json:server": "json-server --watch ./src/assets/mock/db.json --routes ./src/assets/mock/routes.json --port 8000",
    "serve": "ng serve",
    "start": "(trap 'kill 0' INT; npm run json:server & npm run serve)",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage",
    "lint": "eslint --ext .ts,.html .",
    "deploy": "ng deploy --base-href=/activity-tracker/ --name=\"Bruno Ladeia\" --email=bruno13dm@outlook.com",
    "prepare": "husky install"
  },
